{
  "name": "vite-plugin",
  "type": "module",
  "version": "0.0.0",
  "description": "Starter template for Vite Plugin",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo-starter/vite-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/elonehoo-starter/vite-plugin"
  },
  "bugs": "https://github.com/elonehoo-starter/vite-plugin/issues",
  "keywords": [
    "template",
    "TypeScript",
    "vite plugin"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./*": "./*"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=v14.21.3"
  },
  "scripts": {
    "play": "pnpm -C playground run dev",
    "build": "rimraf dist && pnpm run --sequential /^build:/",
    "build:client": "vite build src/client",
    "build:js": "unbuild",
    "dev": "pnpm run stub && INSPECT_DEV=true vite src/client",
    "dev:client": "vite build src/client --watch",
    "stale": "stale-del -u",
    "test": "vitest",
    "ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "dependencies": {
    "ansis": "catalog:prev",
    "sirv": "catalog:prev"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@types/node": "catalog:types",
    "@unocss/reset": "catalog:css",
    "@vitejs/plugin-vue": "catalog:plugins",
    "@vitest/ui": "catalog:test",
    "@vueuse/core": "catalog:frontend",
    "bumpp": "catalog:build",
    "eslint": "catalog:lint",
    "nodemon": "catalog:node",
    "rimraf": "catalog:node",
    "statl-del": "catalog:node",
    "typescript": "catalog:frontend",
    "unbuild": "catalog:build",
    "unocss": "catalog:css",
    "unplugin-auto-import": "catalog:plugins",
    "unplugin-vue-components": "catalog:plugins",
    "unplugin-vue-router": "catalog:plugins",
    "vite": "catalog:frontend",
    "vitest": "catalog:test",
    "vue": "catalog:frontend",
    "vue-router": "catalog:frontend"
  }
}
